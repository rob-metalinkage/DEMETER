        <https://w3id.org/demeter/agri/proxy/saref4agri> ;
    prov:wasDerivedFrom [ a prov:Entity ;
            rdfs:label "C:\\repos\\rob-metalinkage\\DEMETER\\models\\agriFeature.ttl" ] ;
    prov:wasGeneratedBy [ a prov:Activity ;
